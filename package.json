{
  "name": "seneca-logentries-logger",
  "version": "1.0.0",
  "description": "Seneca Logentries Logger",
  "main": "logentries.js",
  "precommit": "test",
  "scripts": {
    "test": "lab -v -P test -L -t 95 $LAB_FLAGS",
    "test:node6": "LAB_FLAGS='--leaks' && npm t",
    "coveralls": "lab -s -P test -r lcov | coveralls",
    "coverage": "lab -v -P test -L -t 95 -r html > coverage.html",
    "annotate": "docco seneca.js logentries.js -o docs/annotated"
  },
  "author": "David Gonzalez",
  "license": "MIT",
  "dependencies": {
    "le_node": "1.6.0"
  },
  "engines": {
    "node": ">=4.x"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "code": "3.0.x",
    "eslint-config-seneca": "3.x.x",
    "eslint-plugin-hapi": "4.x.x",
    "eslint-plugin-standard": "2.x.x",
    "lab": "11.0.x",
    "mock-require": "1.3.0",
    "seneca": "plugin"
  },
  "pre-commit": [
    "test",
    "lint"
  ],
  "files": [
    "logentries.js",
    "README.md",
    "LICENSE"
  ]
}
